{
  "extends": "airbnb",
  "rules": {
    "react/jsx-no-bind": 0,
    "jsx-a11y/img-uses-alt": 0,
    "jsx-a11y/redundant-alt": 0,
    "jsx-a11y/valid-aria-role": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    # "arrow-parens": ["as-needed"],
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
